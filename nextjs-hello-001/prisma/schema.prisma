generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_notice {
  m_seq Int @id @default(autoincrement())
  m_authior String
  m_date String
  m_time String
  m_subject String
  m_content String
}
