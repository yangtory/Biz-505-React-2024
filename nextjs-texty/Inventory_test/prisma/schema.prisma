generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_quest {
  q_quest   String @id
  q_answer1 String
  q_answer2 String
  q_answer3 String
}